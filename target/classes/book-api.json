      "get":{
        "tags":[
          "user-controller"
        ],
        "summary":"getUserById",
        "operationId":"getUserByIdUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/User"
            }
          }
        },
        "deprecated":false
      }
