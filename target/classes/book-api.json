  "paths":{
    "/address/{id}":{
      "get":{
        "tags":[
          "address-controller"
        ],
        "summary":"getAddressById",
        "operationId":"getAddressByIdUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/Address"
            }
          }
        },
        "deprecated":false
      }
    },
    "/api/order/{id}":{
      "get":{
        "tags":[
          "rest-order-controller"
        ],
        "summary":"getOrderById",
        "operationId":"getOrderByIdUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/OrderDTO"
            }
          }
        },
        "deprecated":false
      }
    },
    "/book":{
      "get":{
        "tags":[
          "book-controller"
        ],
        "summary":"getAllBooks",
        "operationId":"getAllBooksUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"isbn",
            "in":"query",
            "description":"isbn",
            "required":false,
            "type":"string"
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/GetAllBooksResponse"
            }
          }
        },
        "deprecated":false
      },
      "post":{
        "tags":[
          "book-controller"
        ],
        "summary":"addBook",
        "operationId":"addBookUsingPOST",
        "consumes":[
          "application/json"
        ],
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "in":"body",
            "name":"book",
            "description":"book",
            "required":true,
            "schema":{
              "$ref":"#/definitions/Book"
            }
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/Book"
            }
          }
        },
        "deprecated":false
      }
    },
    "/book/{id}":{
      "get":{
        "tags":[
          "book-controller"
        ],
        "summary":"Find book by ID",
        "description":"Returns book according to given id",
        "operationId":"getBookByIdUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"Unique Id of Book",
            "required":false,
            "type":"integer",
            "format":"int32",
            "x-example":33
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/Book"
            }
          }
        },
        "deprecated":false
      },
      "put":{
        "tags":[
          "book-controller"
        ],
        "summary":"updateBook",
        "operationId":"updateBookUsingPUT",
        "consumes":[
          "application/json"
        ],
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "in":"body",
            "name":"book",
            "description":"book",
            "required":true,
            "schema":{
              "$ref":"#/definitions/Book"
            }
          },
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
          }
        ],
        "responses":{
          "200":{
            "description":"OK",
            "schema":{
              "$ref":"#/definitions/Book"
            }
          }
        },
        "deprecated":false
      },
      "delete":{
        "tags":[
          "book-controller"
        ],
        "summary":"deleteBook",
        "operationId":"deleteBookUsingDELETE",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
          }
        ],
        "responses":{
          "200":{
            "description":"OK"
          }
        },
        "deprecated":false
      }
    },
    "/orderposition/{id}":{
      "get":{
        "tags":[
          "order-position-controller"
        ],
        "summary":"getOrderPositionById",
        "operationId":"getOrderPositionByIdUsingGET",
        "produces":[
          "*/*"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"id",
            "required":true,
            "type":"integer",
            "format":"int32"
