  "definitions":{
    "Address":{
      "type":"object",
      "properties":{
        "address":{
          "type":"string"
        },
        "city":{
          "type":"string"
        },
        "id":{
          "type":"integer",
          "format":"int32"
        },
        "name":{
          "type":"string"
        },
        "phone":{
          "type":"string"
        },
        "postalCode":{
          "type":"string"
        },
        "surname":{
          "type":"string"
        }
      },
      "title":"Address"
    },
    "Book":{
      "type":"object",
      "properties":{
        "author":{
          "type":"string"
        },
        "id":{
          "type":"integer",
          "format":"int32"
        },
        "isbn":{
          "type":"string"
        },
        "price":{
          "type":"number",
          "format":"double"
        },
        "quantity":{
          "type":"integer",
          "format":"int32"
        },
        "title":{
          "type":"string",
          "description":"book title"
        }
      },
      "title":"Book",
      "description":"Data about book in bookstore"
    },
    "GetAllBooksResponse":{
      "type":"object",
      "properties":{
        "books":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Book"
          }
        }
      },
      "title":"GetAllBooksResponse"
    },
    "OrderDTO":{
      "type":"object",
      "properties":{
        "address":{
          "type":"string"
        },
        "id":{
          "type":"integer",
          "format":"int32"
        },
        "orderPositions":{
          "type":"array",
          "items":{
            "type":"string"
          }
        },
        "status":{
          "type":"string",
          "enum":[
            "NEW",
            "REALIZATION",
            "DELIVERY",
            "DONE"
          ]
        },
        "user":{
          "type":"string"
        }
      },
      "title":"OrderDTO"
    },
    "OrderPositionDTO":{
      "type":"object",
      "properties":{
        "book":{
          "type":"string"
        },
        "id":{
          "type":"integer",
          "format":"int32"
        },
        "positionQuantity":{
          "type":"integer",
          "format":"int32"
        }
      },
      "title":"OrderPositionDTO"
    },
    "User":{
      "type":"object",
      "properties":{
        "id":{
          "type":"integer",
          "format":"int32"
        },
        "login":{
          "type":"string"
        },
        "mail":{
          "type":"string"
        },
        "name":{
          "type":"string"
        },
        "password":{
          "type":"string"
        },
        "surname":{
          "type":"string"
        }
      },
      "title":"User"
    }
  }
}